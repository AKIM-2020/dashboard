<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">


  <changeSet author="k.a.lebedev" id="add user and role test values">

    <insert tableName="roles">
      <column name="id" valueSequenceNext="roles_id_seq"/>
      <column name="name" value="ROLE_USER"/>
    </insert>


    <insert tableName="users">
      <column name="id" valueSequenceNext="users_id_seq"/>
      <column name="user_name" value="test"/>
      <column name="first_name" value="test"/>
      <column name="last_name" value="test"/>
      <column name="email" value="test"/>
      <column name="password" value="$2a$10$jBQeZgN8uEByZ4XHwnV82eRnTbnmm2VvzyXy7zoGgYQhyUmZhsAoe"/>
      <column name="enabled" value="true"/>
    </insert>

    <insert tableName="users_roles">
      <column name="id" valueSequenceNext="users_roles_id_seq"/>
      <column name="user_id" valueSequenceCurrent="users_id_seq"/>
      <column name="role_id" valueSequenceCurrent="roles_id_seq"/>
    </insert>

  </changeSet>

</databaseChangeLog>